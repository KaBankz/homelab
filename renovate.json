{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "ignorePaths": [".template/**"],
  "packageRules": [
    {
      "matchManagers": ["docker-compose"],
      "matchPaths": ["brollic/**"],
      "groupName": "Updates for brollic stack"
    },
    {
      "matchManagers": ["docker-compose"],
      "matchPaths": ["uptime-kuma/**"],
      "groupName": "Updates for uptime-kuma stack"
    },
    {
      "matchManagers": ["docker-compose"],
      "matchPaths": ["monolith/**"],
      "groupName": "Updates for monolith stack"
    },
    {
      "matchManagers": ["docker-compose"],
      "matchPaths": ["dockwatch/**"],
      "groupName": "Updates for dockwatch stack"
    },
    {
      "matchManagers": ["docker-compose"],
      "matchPaths": ["immich/**"],
      "groupName": "Updates for immich stack"
    },
    {
      "matchManagers": ["docker-compose"],
      "matchPaths": ["dockge/**"],
      "groupName": "Updates for dockge stack"
    },
    {
      "matchManagers": ["docker-compose"],
      "matchPaths": ["krabby/**"],
      "groupName": "Updates for krabby stack"
    },
    {
      "matchManagers": ["docker-compose"],
      "matchPaths": ["cloudflared/**"],
      "groupName": "Updates for cloudflared stack"
    },
    {
      "matchManagers": ["docker-compose"],
      "matchPaths": ["speedtest-tracker/**"],
      "groupName": "Updates for speedtest-tracker stack"
    },
    {
      "matchManagers": ["docker-compose"],
      "matchPaths": ["minecraft/**"],
      "groupName": "Updates for minecraft stack"
    },
    {
      "matchManagers": ["docker-compose"],
      "matchPaths": ["dozzle/**"],
      "groupName": "Updates for dozzle stack"
    },
    {
      "matchDatasources": ["docker"],
      "versionCompatibility": "^(?<compatibility>\\w+)-(?<version>.*)$",
      "matchPackageNames": ["/^ghcr.io/hotio//"]
    },
    {
      "matchPackageNames": ["ghcr.io/hotio/qbittorrent"],
      "sourceUrl": "https://github.com/qbittorrent/qbittorrent"
    },
    {
      "matchPackageNames": ["ghcr.io/hotio/qbitmanage"],
      "sourceUrl": "https://github.com/StuffAnThings/qbit_manage"
    },
    {
      "matchPackageNames": ["ghcr.io/hotio/prowlarr"],
      "sourceUrl": "https://github.com/Prowlarr/Prowlarr"
    },
    {
      "matchPackageNames": ["ghcr.io/hotio/sonarr"],
      "sourceUrl": "https://github.com/Sonarr/Sonarr"
    },
    {
      "matchPackageNames": ["ghcr.io/hotio/radarr"],
      "sourceUrl": "https://github.com/Radarr/Radarr"
    },
    {
      "matchPackageNames": ["ghcr.io/hotio/plex"],
      "changelogUrl": "https://forums.plex.tv/t/plex-media-server/30447"
    },
    {
      "matchPackageNames": ["ghcr.io/hotio/tautulli"],
      "sourceUrl": "https://github.com/Tautulli/Tautulli"
    },
    {
      "matchPackageNames": ["lscr.io/linuxserver/speedtest-tracker"],
      "sourceUrl": "https://github.com/alexjustesen/speedtest-tracker"
    }
  ]
}
